<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--SeekArc-->
    <declare-styleable name="SeekArc">
        <attr name="thumb" format="reference" />
        <attr name="thumbOffset" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="progressWidth" format="dimension" />
        <attr name="arcWidth" format="dimension" />
        <attr name="progress" format="integer" />
        <attr name="rotation" format="integer" />
        <attr name="startAngle" format="integer" />
        <attr name="sweepAngle" format="integer" />
        <attr name="arcColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="roundEdges" format="boolean" />
        <attr name="touchInside" format="boolean" />
        <attr name="clockwise" format="boolean" />
        <attr name="enabled" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CarouselPicker">
        <attr name="items_visible" />
    </declare-styleable>
    <attr name="items_visible" format="enum">
        <enum name="three" value="3" />
        <enum name="five" value="5" />
        <enum name="seven" value="7" />
    </attr>

    <!--Horizontal Picker-->
    <declare-styleable name="HorizontalPicker">
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="values" format="reference" />
        <attr name="android:ellipsize" />
        <attr name="android:marqueeRepeatLimit" />
        <attr name="dividerSize" format="dimension" />
        <attr name="sideItems" format="integer" />
    </declare-styleable>
    <attr name="horizontalPickerStyle" format="reference" />
    <declare-styleable name="CategoryView">
    </declare-styleable>
    <declare-styleable name="SubCategoryView">
    </declare-styleable>
    <declare-styleable name="HorizontalOption">
    </declare-styleable>
</resources>